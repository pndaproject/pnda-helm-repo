{
  "hadoop_distro": "none",
  "clustername": "pnda",
  "edge_node": "{{ edge_node }}",
  "user_interfaces": [
    {
      "name": "Hadoop Cluster Manager",
      "link": "{{ hadoop_manager_link }}"
    },
    {
      "name": "Kafka Manager",
      "link": "{{ kafka_manager_link }}"
    },
    {
      "name": "Grafana",
      "link": "http://grafana.pnda.io/"
    },
    {
      "name": "PNDA logserver",
      "link": "{{ kibana_link }}"
    },
    {
      "name": "Jupyter",
      "link": "http://notebooks.pnda.io/"
    }
  ],
  "machine_interfaces" : {
      "HTTPFS" :  "{{ httpfs_link }}"
  },
  "frontend": {
    "version": "develop"
  },
  "backend": {
    "data-manager": {
      "version": "develop",
      "host": "console-backend-manager", "port": "3123"
    }
  },
  "login_mode": "PAM"
}
